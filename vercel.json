{
  "buildCommand": "npm run build:web",
  "outputDirectory": "web-build",
  "framework": "expo",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://fixr-node-api.onrender.com/api/$1"
    },
    {
      "source": "/ai/(.*)",
      "destination": "https://fixr-python-api.onrender.com/ai/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
} 